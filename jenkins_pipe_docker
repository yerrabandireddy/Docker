pipeline{
    agent any
    parameters{
        choice(name: "docker", choices: "docker\nabc", descriprion: "building my docker image")
        string(name: "workspace", description: "give some name")
    }
    stages{
        stage("cloaning"){
        steps{
            checkout([$class: 'GitScm',
            branches: [name: "origin/master"],
            userRemoteConfigs: [url: " https://github.com/yerrabandireddy/Docker.git"]])
        }
        }
        
    }
}    
